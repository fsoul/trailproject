<script>
  $( document ).ready(function() {
    // Disables the index dropdown if it's not force.com
    $('#select_subject').change(function() {
        if (($(this).val().indexOf('Force.com') != -1) &#124;&#124; ($(this).val().indexOf('All Services') != -1)){
          $('#select_type').prop('disabled',false);
      } else {
        $('#select_type').prop('disabled',true);
      }
    });
    var percent = 0.40;
    var add_width = (percent*window.innerHeight);
    $("#open-console_btn").toggle(function(){
      $("#open-console_btn").text('x');
      $('#api-console').attr('scrolling','yes');
      $('#api-console').animate({height: add_width},200);
    },function(){
      $("#open-console_btn").text('+');
      $('#api-console').attr('scrolling','no');
      $('#api-console').animate({height: 40 },200);
    });
    changeTocIconState();
    // renderToc();
    var lastSlashIndex = location.pathname.lastIndexOf("/");
    var lastSlash = lastSlashIndex + 1;
    var pageURL = location.pathname.substr(lastSlash);
    var pageName = pageURL.substr(0, pageURL.lastIndexOf("."));
    if( pageName != undefined && pageName != ''){
      var foundLi = $("div#" + pageName);
      foundLi = $("a[href~='" + pageURL + "']");
      $("a[href~='" + pageURL + "']").parent("div").toggleClass('active');
      $("a[href~='" + pageURL + "']").parents('div.collapse').collapse('show');
    }
  });
</script>
