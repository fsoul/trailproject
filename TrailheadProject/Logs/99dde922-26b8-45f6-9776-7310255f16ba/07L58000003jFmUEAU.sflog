<script>
    // new code for handing tabs
    var tabs = $('[data-toggle="tab"]');
    tabs
      .on('click', function (e) {
        e.preventDefault();
        tabs.removeClass('active');
        $(this).tab('show').addClass('active');
      })
      .first()
      .trigger('click');
  
    var checkbox_state = $('#are_badges_public').is(':checked');
    $('#update_error').hide();
  
    $('#are_badges_public').change(function(){
      var promise = $.ajax("/users/profiles/005F0000006u5DxIAI", {
        type: 'PUT',
        data: { "are_badges_public": $('#are_badges_public').is(':checked') }
      });
  
      promise.then(
        function(data){
          $('#update_error').hide();
          checkbox_state = $('#are_badges_public').is(':checked');
        },
        function(err) {
          $('#update_error').show();
          $('#are_badges_public').attr('checked', checkbox_state);
          checkbox_state = $('#are_badges_public').is(':checked');
        });
    });
</script>

